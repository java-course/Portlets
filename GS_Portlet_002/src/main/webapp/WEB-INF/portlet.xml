<?xml version='1.0' encoding='UTF-8' ?>
<portlet-app xmlns='http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd'
             xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
             xsi:schemaLocation='http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd
             http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd' version='2.0'>
    <portlet>
        <description>PortletExample2</description>
        <portlet-name>PortletExample2</portlet-name>
        <display-name>PortletExample2</display-name>
        <portlet-class>com.geminisystems.PortletExample2</portlet-class>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>VIEW</portlet-mode>
            <portlet-mode>EDIT</portlet-mode>
            <portlet-mode>HELP</portlet-mode>
            <portlet-mode>config</portlet-mode>
        </supports>
        <resource-bundle>com.geminisystems.messages</resource-bundle>
        <portlet-info>
            <title>PortletExample2</title>
            <short-title>PortletExample2</short-title>
        </portlet-info>
        <portlet-preferences>
            <preference>
                <name>email</name>
                <value>weqr</value>
            </preference>
            <preference>
                <name>password</name>
                <value>432534</value>
            </preference>
        </portlet-preferences>
    </portlet>
    <custom-portlet-mode>
        <portlet-mode>config</portlet-mode>
    </custom-portlet-mode>
</portlet-app>
